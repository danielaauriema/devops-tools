[Interface]
PrivateKey = {{ wg_client_pvt_key }}
Address = {{ wg_client_addr }}/32
{% if not wg_client_dns is none %}
DNS = {{ wg_client_dns }}
{% endif %}

[Peer]
PublicKey = {{ wg_server_pub_key }}
PresharedKey = {{ wg_client_psk }}
AllowedIPs = {{ wg_client_allowed_ips }}
Endpoint = {{ wg_server_external_addr }}:{{ wg_server_port }}
PersistentKeepalive = {{ wg_client_persistent_keepalive }}
